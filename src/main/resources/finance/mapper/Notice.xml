<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.answerlyl.myfinance.mapper.NoticeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.answerlyl.myfinance.entity.Notice">
        <id column="NOTICE_ID" property="noticeId" />
        <result column="SHOW_DATE" property="showDate" />
        <result column="SHOW_INFO" property="showInfo" />
        <result column="BKG_COLOR" property="bkgColor" />
        <result column="FONT_COLOR" property="fontColor" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        NOTICE_ID, SHOW_DATE, SHOW_INFO, BKG_COLOR, FONT_COLOR
    </sql>

</mapper>
